    "import pandas as pd\n",
    "\n",
    "df = starts.copy()\n",
    "df = df.sort_values(by=[\"record_id\", \"finish_time\"])\n",
    "df = df[df['finish_time'] > df['finish_time'].median()]\n",
    "\n",
    "# Считаем разницу с предыдущим и следующим участником\n",
    "df[\"diff_prev\"] = df.groupby(\"record_id\")[\"finish_time\"].diff().abs()\n",
    "df[\"diff_next\"] = df.groupby(\"record_id\")[\"finish_time\"].diff(-1).abs()\n",
    "\n",
    "# Определяем тех, кто бежал группами (разница <= 5 сек)\n",
    "df[\"within_5sec\"] = ((df[\"diff_prev\"] <= 5) | (df[\"diff_next\"] <= 5))\n",
    "\n",
    "# Число участников в группах\n",
    "group_counts = (df.groupby(\"record_id\")[\"within_5sec\"]\n",
    "                .sum()\n",
    "                .reset_index(name=\"count_in_groups\"))\n",
    "\n",
    "# Общее число участников\n",
    "runner_counts = (df.groupby(\"record_id\")[\"athlete_id\"]\n",
    "                 .nunique()\n",
    "                 .reset_index(name=\"cnt_runners\"))\n",
    "\n",
    "# Объединяем в одну таблицу\n",
    "result = group_counts.merge(runner_counts, on=\"record_id\", how=\"left\")\n",
    "\n",
    "# Добавляем столбец соотношения count_in_groups к cnt_runners\n",
    "result[\"ratio_in_groups\"] = result[\"count_in_groups\"] / result[\"cnt_runners\"]\n",
    "\n",
    "# Считаем среднее ratio_in_groups для пробежек с числом участников от 2 до 46\n",
    "mean_ratio = result[(result['cnt_runners'] >= 46) & (result['cnt_runners'] <= 138)][\"ratio_in_groups\"].mean()\n",
    "print(mean_ratio)\n"
   ]
