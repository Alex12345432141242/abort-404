    "import math\n",
    "\n",
    "# Пример данных\n",
    "wilcoxon_results = {\n",
    "    \"Маршал\": {\"W\": 20, \"n\": 15},\n",
    "    \"Пейсер\": {\"W\": 5, \"n\": 10},\n",
    "    \"Хранение и доставка оборудования\": {\"W\": 8, \"n\": 12},\n",
