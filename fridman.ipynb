    "import pandas as pd\n",
    "from scipy.stats import wilcoxon\n",
    "from itertools import combinations\n",
    "\n",
    "def wilcoxon_test(df, col1, col2, alpha=0.05):\n",
    "    \"\"\"\n",
    "    Выполняет тест Уилкоксона для двух зависимых выборок.\n",
    "    \n",
    "    :param df: DataFrame, содержащий количественные данные.\n",
    "    :param col1: Имя первого столбца.\n",
    "    :param col2: Имя второго столбца.\n",
    "    :param alpha: Уровень значимости для проверки гипотезы.\n",
    "    :return: Статистика, p-value и вывод по гипотезе.\n",
    "    \"\"\"\n",
    "    # Извлекаем данные двух выборок\n",
    "    data1 = df[col1]\n",
    "    data2 = df[col2]\n",
    "    \n",
    "    # Применяем тест Уилкоксона\n",
    "    stat, p_value = wilcoxon(data1, data2)\n",
    "    \n",
    "    # Проверка гипотезы\n",
    "    if p_value < alpha:\n",
    "        result = \"Различия есть\"\n",
    "    else:\n",
    "        result = \"Различий нет\"\n",
    "    \n",
    "    return stat, p_value, result\n",
    "\n",
    "def pairwise_wilcoxon_tests(df, alpha=0.05):\n",
    "    \"\"\"\n",
    "    Выполняет попарные сравнения всех столбцов с использованием теста Уилкоксона.\n",
    "    \n",
    "    :param df: DataFrame, содержащий количественные данные.\n",
    "    :param alpha: Уровень значимости для проверки гипотезы.\n",
    "    :return: DataFrame с результатами.\n",
    "    \"\"\"\n",
    "    results = []\n",
    "    columns = df.columns\n",
    "    \n",
    "    # Все возможные пары столбцов\n",
    "    for col1, col2 in combinations(columns, 2):\n",
    "        stat, p_value, result = wilcoxon_test(df, col1, col2, alpha)\n",
    "        results.append({\n",
    "            'Column 1': col1,\n",
    "            'Column 2': col2,\n",
    "            'Statistic': stat,\n",
    "            'p-value': round(p_value, 4),  # Округляем p-value\n",
    "            'Result': result\n",
    "        })\n",
    "    \n",
    "    # Преобразуем результаты в DataFrame\n",
    "    return pd.DataFrame(results)\n",
    "\n",
    "# Пример применения\n",
    "alpha = 0.05  # Уровень значимости\n",
    "pairwise_results = pairwise_wilcoxon_tests(comeback_newbies, alpha)\n",
    "\n",
    "# Сохранение результатов\n",
    "pairwise_results.to_csv('pairwise_wilcoxon_results.csv', index=False)\n",
    "\n",
    "# Вывод первых строк для проверки\n",
    "print(pairwise_results.head())\n"
   ]
